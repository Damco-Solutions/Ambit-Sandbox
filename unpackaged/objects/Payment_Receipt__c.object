    <validationRules>
        <fullName>Instrument_Date_is_mandatory</fullName>
        <active>true</active>
        <description>Instrument_Date_is_mandatory if Instrument Type is not cash</description>
        <errorConditionFormula>AND(!ISPickval( Instrument_Type__c ,&apos;Cash&apos;), ISBLANK( Instrument_Date__c ))</errorConditionFormula>
        <errorMessage>Instrument Date is Required To Be Filled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Instrument_Type_selected_as_Cheque_or_DD</fullName>
        <active>true</active>
        <description>If Cheque / DD then Number  should only enter in Instrument No field</description>
        <errorConditionFormula>AND(ISPICKVAL( Instrument_Type__c ,&apos;Cheque&apos;),(REGEX(  Instrument_No__c,&quot;^[a-z A-Z]*$&quot;)))</errorConditionFormula>
        <errorMessage>Instrument No should be only numbers</errorMessage>
    </validationRules>
