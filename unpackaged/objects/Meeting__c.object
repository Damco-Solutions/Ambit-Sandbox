        <fullName>In_Progress_Completed_Meetings</fullName>
        <active>true</active>
        <errorConditionFormula>OR(ISPICKVAL(Status__c, &apos;Completed&apos;), ISPICKVAL(Status__c, &apos;In Progress&apos;))
&amp;&amp; ISCHANGED(Salesperson__c)</errorConditionFormula>
        <errorMessage>In Progress/Completed Meetings cannot be re-assigned</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Person_Met_and_Meeting_Outcome_can_only</fullName>
        <active>false</active>
        <errorConditionFormula>AND(OR(ISPICKVAL (Status__c,&quot;In Progress&quot;),ISPICKVAL (Status__c, &quot;Completed&quot; )),
((ISBLANK(Person_Met__c))||(ISPICKVAL(Meeting_Outcome__c,&apos;&apos;))
))</errorConditionFormula>
        <errorMessage>Person Met and Meeting Outcome can only be added for &apos;In Progress&apos;/&apos;Completed&apos; meetings</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Scheduled_End_Time_Should_be_Greater</fullName>
        <active>true</active>
        <errorConditionFormula>Scheduled_End_Time__c &lt;=  Scheduled_Start_Time__c</errorConditionFormula>
        <errorMessage>Scheduled End Time Should be Greater than Scheduled Start Time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Scheduled_Start_Time_Should_be_Greater</fullName>
        <active>true</active>
        <errorConditionFormula>(Scheduled_Start_Time__c &lt; NOW() ) &amp;&amp;
ISNEW()</errorConditionFormula>
        <errorMessage>Scheduled Start Time Should be Greater than Equal to Current Time</errorMessage>
    </validationRules>
    <validationRules>
