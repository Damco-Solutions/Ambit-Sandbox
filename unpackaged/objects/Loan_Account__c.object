        <fullName>DSA_Connector_For_Unsecured_Loans</fullName>
        <active>true</active>
        <description>DSA/Connector is mandatory for Unsecured loans</description>
        <errorConditionFormula>(ISPICKVAL(Applicant__r.Product__c,&apos;Lead - UDYAM (UNSECURED LOAN)&apos;) ||
ISPICKVAL(Applicant__r.Product__c,&apos;Lead - Rural Udyam&apos;)) &amp;&amp;
ISBLANK(DSA_Connector__c)</errorConditionFormula>
        <errorMessage>Please provide DSA/Connector</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_For_Not_Interested_In_Top_up</fullName>
        <active>true</active>
        <description>if Not_Interested_In_Top_up is check , Reason For Not Interested In Top up should not be blank.</description>
        <errorConditionFormula>AND(Not_Interested_for_Top_Up__c, ISBLANK(TEXT( Reason_For_Not_Interested_In_Top_up__c )))</errorConditionFormula>
        <errorDisplayField>Reason_For_Not_Interested_In_Top_up__c</errorDisplayField>
        <errorMessage>Please enter the reason for decline</errorMessage>
    </validationRules>
