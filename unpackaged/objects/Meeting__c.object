        <active>false</active>
        <errorConditionFormula>ISPICKVAL(Status__c,&apos;Re-Scheduled&apos;) &amp;&amp; TIMEVALUE(Scheduled_Start_Time__c)&lt;  TIMENOW()</errorConditionFormula>
